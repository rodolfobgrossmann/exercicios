#include <stdio.h>

int main()
{
    int i, max = 100;
    int fizz = 3;
    int buzz = 5;
    for( i=1 ; i <= max ; i++ ){
        if( i % fizz == 0 ){
            if ( i % buzz == 0){
                printf( "fizzbuzz\n" );
            }else {
                printf( "fizz\n" );
            }
        }else if ( i % buzz == 0 ){
            printf( "buzz\n" );    
        }else{
            printf( "%d\n", i );
        }
    }
}
